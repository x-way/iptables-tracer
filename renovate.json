{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base",
    ":disableDependencyDashboard"
  ],
  "assignees": [
    "x-way"
  ],
  "reviewers": [
    "x-way"
  ],
  "labels": [
    "renovate"
  ],
  "postUpdateOptions": [
    "gomodTidy"
  ],
  "packageRules": [
    {
      "packageNames": [
        "github.com/x-way/iptables-tracer"
        "go4.org/netipx",
        "golang.org/x/crypto/x509roots/fallback",
        "golang.org/x/exp",
      ],
      "updateTypes": [
        "patch",
        "digest"
      ],
      "reviewers": [],
      "assignees": [],
      "automerge": true
    },
    {
      "packageNames": [
        "golang.org/x/net"
        "golang.org/x/sync",
        "golang.org/x/sys",
        "golang.org/x/time",
      ],
      "updateTypes": [
        "patch",
        "minor"
      ],
      "reviewers": [],
      "assignees": [],
      "automerge": true
    },
    {
      "updateTypes": [
        "patch"
      ],
      "reviewers": [],
      "assignees": [],
      "automerge": true
    }
  ]
}
